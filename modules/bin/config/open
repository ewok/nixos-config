#!/usr/bin/env bash

if [[ "$WSLENV" != "" ]]; then
    explorer.exe "$@"
elif [[ "$OSTYPE" == "linux-gnu" ]]; then
    xdg-open "$@"
elif [[ "$OSTYPE" == "darwin"* ]]; then
    /usr/bin/open "$@"
elif [[ "$OSTYPE" == "cygwin" ]]; then
    exit 1
elif [[ "$OSTYPE" == "msys" ]]; then
    exit 1
elif [[ "$OSTYPE" == "freebsd"* ]]; then
    exit 1
else
    exit 1
fi
